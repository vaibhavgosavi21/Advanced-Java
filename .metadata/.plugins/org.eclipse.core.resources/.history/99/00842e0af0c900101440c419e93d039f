package hibernate2;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import lombok.AllArgsConstructor;
import lombok.Data;

@Entity
@Data
@AllArgsConstructor
public class StudentDTO {

	public StudentDTO() {
		// TODO Auto-generated constructor stub
	}
	@Id
	private int rollno;
	private String sname;
	private double marks;
}
